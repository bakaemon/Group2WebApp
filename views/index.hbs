<!-- Website Overview -->
<div class="panel panel-default">
  <div class="panel-heading main-color-bg">
    <h3 class="panel-title">Website Overview</h3>
  </div>
  <div class="panel-body">
    <div class="col-md-3">
      <div class="well dash-box">
        <h2><span class="glyphicon glyphicon-user" aria-hidden="true"></span> {{numsOfUsers}}</h2>
        <h4>Users</h4>
      </div>
    </div>
    <div class="col-md-3">
      <div class="well dash-box">
        <h2><span class="glyphicon glyphicon-pencil" aria-hidden="true"></span> {{numsOfCourse}}</h2>
        <h4>Course</h4>
      </div>
    </div>
    <div class="col-md-3">
      <div class="well dash-box">
        <h2><span class="glyphicon glyphicon-list" aria-hidden="true"></span> {{numsOfCategory}}</h2>
        <h4>Categories</h4>
      </div>
    </div>
    {{!-- <div class="col-md-3">
      <div class="well dash-box">
        <h2><span class="glyphicon glyphicon-stats" aria-hidden="true"></span> 12,334</h2>
        <h4>Visitors</h4>
      </div>
    </div> --}}
  </div>
</div>

<!-- Latest Users -->
<div class="panel panel-default">
  <div class="panel-heading main-color-bg">
    <h3 class="panel-title">Top 10 trainees of the year.</h3>
  </div>
  <br/>
  <div class="panel-body">
    <table class="table table-striped table-hover">
      <tbody>
        <tr>
          <th>No.</th>
          <th>Name</th>
          <th>Email</th>
          <th>Score</th>
        </tr>
        {{#each user_data}}
        <tr>
          <td>#{{math @index "+" 1}}</td>
          <td>{{username}}</td>
          <td>{{email}}</td>
          <td>{{bio.Score}}</td>
        </tr>
        {{/each}}
      </tbody>
    </table>
  </div>
</div>